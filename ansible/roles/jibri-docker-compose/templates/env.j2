CONFIG=~/.jitsi-meet-cfg
JITSI_IMAGE_VERSION={{ jibri_docker_version }}
JIBRI_INSTANCE_ID={{ jibri_instance_id }}
JIBRI_XMPP_USER={{ jibri_auth_control_user }}
JIBRI_XMPP_PASSWORD={{ jibri_auth_control_pass }}
JIBRI_BREWERY_MUC={{ jibri_xmpp_room_name }}
JIBRI_RECORDER_USER={{ jibri_auth_call_user }}
JIBRI_RECORDER_PASSWORD={{ jibri_auth_call_pass }}
XMPP_DOMAIN={{ environment_domain_name }}
XMPP_AUTH_DOMAIN=auth.{{ environment_domain_name }}
XMPP_INTERNAL_MUC_DOMAIN=internal.auth.{{ environment_domain_name }}
XMPP_HIDDEN_DOMAIN=recorder.{{ environment_domain_name }}
XMPP_SERVER={{ environment_domain_name }}
XMPP_PORT=5222
JIBRI_WEBHOOK_SUBSCRIBERS=http://{{ ansible_default_ipv4.address }}:6000/hook
JIBRI_FINALIZE_RECORDING_SCRIPT_PATH=/usr/bin/jitsi_uploader.sh
FAILED_UPLOAD_DIR=/config/recordings/failed

    location ~ ^/colibri-ws/jvb-({{ jitsi_meet_colibri_proxy_second_octet_regexp }})-({{ jitsi_meet_colibri_proxy_third_octet_regexp }})-({{ jitsi_meet_colibri_proxy_fourth_octet_regexp }})(/?)(.*) {
